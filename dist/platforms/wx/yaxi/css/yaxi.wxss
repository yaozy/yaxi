/*隐藏滚动条*/
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
.yx-control {
  overflow: hidden;
  box-sizing: border-box;
  flex-grow: 0;
  flex-shrink: 0;
  -webkit-overflow-scrolling: touch;
  -webkit-appearance: none;
  -webkit-tap-highlight-color: transparent;
  -webkit-font-smoothing: antialiased;
}
.yx-box {
  position: relative;
  display: block;
  height: auto;
}
.yx-databox {
  position: relative;
  display: block;
  overflow: auto;
  width: auto;
  height: auto;
}
.yx-dataempty {
  position: absolute;
  display: inline-block;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  text-align: center;
  color: #616161;
}
.yx-dataempty > .yx-icon {
  display: block;
  font-size: 80rpx;
}
.yx-dataempty-text {
  display: block;
  margin-top: 10rpx;
  white-space: nowrap;
}
.yx-icon {
  overflow: hidden;
  display: inline-flex;
  justify-content: center;
  align-items: center;
  font-family: "iconfont";
  font-size: 50rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.yx-iconbutton {
  display: inline-flex;
  width: 150rpx;
  height: 100rpx;
  overflow: hidden;
  flex-direction: column;
  justify-items: center;
  justify-content: center;
  align-items: center;
}
.yx-iconbutton-icon {
  overflow: hidden;
  display: inline-flex;
  justify-content: center;
  align-items: center;
  flex-grow: 0;
  flex-shrink: 0;
  font-family: "iconfont";
  font-size: 50rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.yx-iconbutton-content {
  white-space: nowrap;
}
.yx-imagebutton {
  display: inline-flex;
  width: 150rpx;
  height: 100rpx;
  overflow: hidden;
  flex-direction: column;
  justify-items: center;
  justify-content: center;
  align-items: center;
  text-align: center;
}
.yx-imagebutton-image {
  width: 50rpx;
  height: 50rpx;
  margin-top: 10rpx;
  flex-grow: 0;
  flex-shrink: 0;
  text-align: center;
  line-height: 1.2;
  font-size: 60rpx;
  background-position: center;
  background-size: contain;
}
.yx-imagebutton-content {
  white-space: nowrap;
}
.yx-line {
  display: block;
  width: 100%;
  height: 5rpx;
}
.yx-vline {
  display: block;
  width: 5rpx;
  height: 100%;
}
.yx-loading {
  position: relative;
  display: inline-block;
  width: 80rpx;
  height: 80rpx;
  color: #9e9e9e;
  font-family: "iconfont" !important;
  font-size: 20rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.yx-loading-center {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}
.yx-loading-dot {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
.yx-loading-dot::before {
  display: block;
  width: 20%;
  height: 20%;
  margin: 0 auto;
  color: inherit;
  font-size: inherit;
  animation: yx-loading 1.2s infinite ease-in-out both;
}
.yx-loading-dot2 {
  transform: rotate(30deg);
}
.yx-loading-dot3 {
  transform: rotate(60deg);
}
.yx-loading-dot4 {
  transform: rotate(90deg);
}
.yx-loading-dot5 {
  transform: rotate(120deg);
}
.yx-loading-dot6 {
  transform: rotate(150deg);
}
.yx-loading-dot7 {
  transform: rotate(180deg);
}
.yx-loading-dot8 {
  transform: rotate(210deg);
}
.yx-loading-dot9 {
  transform: rotate(240deg);
}
.yx-loading-dot10 {
  transform: rotate(270deg);
}
.yx-loading-dot11 {
  transform: rotate(300deg);
}
.yx-loading-dot12 {
  transform: rotate(330deg);
}
.yx-loading-dot2::before {
  animation-delay: -1.1s;
}
.yx-loading-dot3::before {
  animation-delay: -1s;
}
.yx-loading-dot4::before {
  animation-delay: -0.9s;
}
.yx-loading-dot5::before {
  animation-delay: -0.8s;
}
.yx-loading-dot6::before {
  animation-delay: -0.7s;
}
.yx-loading-dot7::before {
  animation-delay: -0.6s;
}
.yx-loading-dot8::before {
  animation-delay: -0.5s;
}
.yx-loading-dot9::before {
  animation-delay: -0.4s;
}
.yx-loading-dot10::before {
  animation-delay: -0.3s;
}
.yx-loading-dot11::before {
  animation-delay: -0.2s;
}
.yx-loading-dot12::before {
  animation-delay: -0.1s;
}
@keyframes yx-loading {
  0%,
  80%,
  100% {
    transform: scale(0);
  }
  40% {
    transform: scale(1);
  }
}
.yx-marquee {
  position: relative;
  height: 60rpx;
}
.yx-marquee-content {
  position: absolute;
  top: 50%;
  height: auto;
  white-space: nowrap;
  transform: translateY(-50%);
}
.yx-marquee-content > .yx-control {
  display: inline-block;
  height: 100%;
  line-height: 1;
}
@keyframes marquee {
  from {
    transform: translate(0, -50%);
  }
  to {
    transform: translate(-50%, -50%);
  }
}
.yx-masklayer {
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  width: 100%;
  height: auto;
  background-color: #000000;
  opacity: 0.3;
  z-index: -1;
}
.yx-scrollbox {
  overflow: auto;
}
.yx-slidebox {
  position: relative;
  overflow: hidden;
  height: 300rpx;
}
.yx-slidebox-body {
  position: absolute;
  overflow: visible;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  display: -webkit-flex;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
}
.yx-slidebox-body > .yx-control {
  overflow: auto;
  width: 100%;
  height: 100%;
}
.yx-noscroll .yx-box,
.yx-noscroll .yx-slidebox-body {
  overflow: hidden !important;
}
.yx-stackbox {
  position: relative;
  overflow: hidden;
}
.yx-stackbox-body {
  position: absolute;
  overflow: visible;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  display: -webkit-flex;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
}
.yx-stackbox-body > .yx-control {
  overflow: auto;
  width: 100%;
  height: 100%;
}
.yx-switchbox {
  position: relative;
  overflow: hidden;
}
.yx-switchbox > .yx-control {
  overflow: auto;
  width: 100%;
}
.yx-tabbar {
  display: -webkit-flex;
  display: flex;
  justify-items: stretch;
  height: 100rpx;
  font-size: 30rpx;
  overflow: hidden;
}
.yx-text {
  display: inline-block;
}
.yx-textbox {
  display: inline-block;
  width: 400rpx;
  height: 80rpx;
  line-height: 80rpx;
  border: 0 solid #78909c;
  border-width: 0 0 0.5px 0;
  padding: 0 10rpx;
  outline: none;
  font-size: 32rpx;
}
.yx-button {
  display: inline-flex;
  justify-items: center;
  justify-content: center;
  align-content: center;
  align-items: center;
  overflow: hidden;
  width: 100%;
  height: 80rpx;
  border-radius: 0;
  padding: 0 20rpx;
  white-space: nowrap;
  background-color: #1c86ee;
  color: #ffffff;
  font-size: inherit;
}
.yx-button:active {
  background-color: #5eaadd;
}
.yx-checkbox {
  display: inline-flex;
  overflow: hidden;
  align-content: center;
  align-items: center;
}
.yx-checkbox-content {
  white-space: nowrap;
}
.yx-memo {
  display: inline-block;
  width: 400rpx;
  height: 200rpx;
  border: 1px solid #263238;
  padding: 8rpx;
  background-color: #ffffff;
}
.yx-memo > textarea {
  border: 0;
  width: 100%;
  height: 100%;
  background-color: #ffffff;
  outline: none;
  resize: none;
}
.yx-number {
  position: relative;
}
.yx-number > input {
  padding: 0 4rpx;
}
.yx-number > span {
  display: none;
}
.yx-number[button] {
  padding: 0 60rpx;
}
.yx-number[button] > span {
  display: inline-block;
}
.yx-number-plus,
.yx-number-minus {
  position: absolute;
  display: inline-block;
  width: 60rpx;
  height: 100%;
  text-align: center;
  font-family: serif;
  font-weight: lighter;
  color: #37474f;
  cursor: default;
}
.yx-number-plus {
  right: 0;
  border-left: 1px solid #263238;
  font-size: 32rpx;
}
.yx-number-minus {
  left: 0;
  border-right: 1px solid #263238;
  font-size: 40rpx;
}
.yx-number-plus[disabled],
.yx-number-minus[disabled] {
  color: #9e9e9e;
}
.yx-password {
  position: relative;
}
.yx-password > input {
  padding-right: 60rpx;
}
.yx-password > span {
  position: absolute;
  display: inline-block;
  top: 0;
  right: 0;
  bottom: 0;
  width: 60rpx;
}
.yx-password > span > svg {
  position: absolute;
  width: 1.2em;
  height: 1.2em;
  top: 50%;
  right: 1rpx;
  margin-top: -0.6em;
}
.yx-radio {
  display: inline-flex;
  overflow: hidden;
  align-content: center;
  align-items: center;
}
.yx-radio-content {
  white-space: nowrap;
}
.yx-switchbutton {
  position: relative;
  display: inline-block;
  width: 200rpx;
  height: 60rpx;
}
.yx-switchbutton-bar {
  position: absolute;
  display: inline-block;
  top: 50%;
  left: 0;
  right: 0;
  height: 50rpx;
  margin-top: -25rpx;
  border-radius: 25rpx;
  background-color: #9e9e9e;
}
.yx-switchbutton-button {
  position: absolute;
  display: inline-block;
  left: 0;
  top: 50%;
  width: 50rpx;
  height: 50rpx;
  margin-top: -25rpx;
  border: 1px solid #263238;
  border-radius: 25rpx;
  background-color: white;
}
.yx-switchbutton-checked > .yx-switchbutton-bar {
  background-color: #71c04a;
}
.yx-switchbutton-checked > .yx-switchbutton-button {
  left: auto;
  right: 0;
  transition: right 1s;
}
.yx-dialog {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 999;
  overflow: hidden;
}
.yx-host {
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  width: 100%;
  height: auto;
  font-family: "Helvetica Neue", Helvetica, STHeiTi, Arial, sans-serif;
  font-size: 32rpx;
}
.yx-page {
  position: absolute;
  display: -webkit-flex;
  display: flex;
  flex-direction: column;
  overflow: hidden;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 1;
  background-color: #ffffff;
  color: #212121;
}
.yx-absolute-left {
  position: absolute;
  left: 0;
}
.yx-absolute-top {
  position: absolute;
  top: 0;
}
.yx-absolute-right {
  position: absolute;
  right: 0;
}
.yx-absolute-bottom {
  position: absolute;
  bottom: 0;
}
.yx-absolute-center {
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
}
.yx-absolute-middle {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
}
.yx-absolute-center.yx-absolute-middle {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}
.yx-static {
  position: static;
}
.yx-layout-row {
  display: -webkit-flex;
  display: flex;
  flex-direction: row;
}
.yx-layout-row-reverse {
  display: -webkit-flex;
  display: flex;
  flex-direction: row-reverse;
}
.yx-layout-column {
  display: -webkit-flex;
  display: flex;
  flex-direction: column;
}
.yx-layout-column-reverse {
  display: -webkit-flex;
  display: flex;
  flex-direction: column-reverse;
}
.yx-layout-before {
  justify-items: flex-start;
  justify-content: flex-start;
}
.yx-layout-center {
  justify-items: center;
  justify-content: center;
}
.yx-layout-after {
  justify-items: flex-end;
  justify-content: flex-end;
}
.yx-layout-space-between {
  justify-items: space-between;
  justify-content: space-between;
}
.yx-layout-space-around {
  justify-items: space-around;
  justify-content: space-around;
}
.yx-layout-stretch {
  justify-items: stretch;
  justify-content: stretch;
}
.yx-layout-left,
.yx-layout-up {
  align-content: flex-start;
  align-items: flex-start;
}
.yx-layout-middle {
  align-content: center;
  align-items: center;
}
.yx-layout-right,
.yx-layout-down {
  align-content: flex-end;
  align-items: flex-end;
}
.yx-layout-baseline {
  align-content: baseline;
  align-items: baseline;
}
