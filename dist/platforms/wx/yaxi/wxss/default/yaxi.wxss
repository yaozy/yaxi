.yx-control {
  box-sizing: border-box;
  flex-grow: 0;
  flex-shrink: 0;
  -webkit-overflow-scrolling: touch;
  -webkit-appearance: none;
  -webkit-tap-highlight-color: transparent;
  -webkit-font-smoothing: antialiased;
}
.yx-box {
  position: relative;
  display: block;
  overflow: auto;
  height: auto;
}
.yx-button {
  display: inline-flex;
  justify-items: center;
  justify-content: center;
  align-content: center;
  align-items: center;
  overflow: hidden;
  width: 100%;
  height: 80rpx;
  white-space: nowrap;
  background-color: #1c86ee;
  color: #f7f7f7;
}
.yx-button.active {
  background-color: #5eaadd;
}
.yx-databox {
  position: relative;
  display: block;
  overflow: auto;
  width: auto;
  height: auto;
}
.yx-icon {
  display: inline-block;
  overflow: hidden;
  text-align: center;
}
.iconfont {
  font-size: 50rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.yx-iconbutton {
  display: inline-flex;
  width: 150rpx;
  height: 100rpx;
  overflow: hidden;
  flex-direction: column;
  justify-items: center;
  justify-content: center;
  align-items: center;
  text-align: center;
}
.yx-iconbutton-icon {
  flex-grow: 0;
  flex-shrink: 0;
  text-align: center;
}
.yx-iconbutton-content {
  font-size: 28rpx;
  white-space: nowrap;
}
.yx-imagebutton {
  display: inline-flex;
  width: 150rpx;
  height: 100rpx;
  overflow: hidden;
  flex-direction: column;
  justify-items: center;
  justify-content: center;
  align-items: center;
  text-align: center;
}
.yx-imagebutton-image {
  width: 50rpx;
  height: 50rpx;
  margin-top: 10rpx;
  flex-grow: 0;
  flex-shrink: 0;
  text-align: center;
  line-height: 1.2;
  font-size: 60rpx;
  background-position: center;
  background-size: contain;
}
.yx-imagebutton-content {
  font-size: 28rpx;
  white-space: nowrap;
}
.yx-line {
  display: block;
  width: 100%;
  height: 5rpx;
}
.yx-vline {
  display: block;
  width: 5rpx;
  height: 100%;
}
.yx-marquee {
  position: relative;
  height: 60rpx;
}
.yx-marquee-content {
  position: absolute;
  top: 50%;
  height: auto;
  white-space: nowrap;
  transform: translateY(-50%);
}
.yx-marquee-content > .yx-control {
  display: inline-block;
  height: 100%;
  line-height: 1;
}
@keyframes marquee {
  from {
    transform: translate(0, -50%);
  }
  to {
    transform: translate(-50%, -50%);
  }
}
.yx-masklayer {
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  width: 100%;
  background-color: #000000;
  opacity: 0.3;
}
.yx-modelbox {
  position: relative;
  display: block;
  overflow: auto;
  width: auto;
  height: auto;
}
.yx-swiper {
  height: 300rpx;
}
.yx-swiper-host,
.yx-swiper-item > .yx-control {
  width: 100%;
  height: 100%;
}
.yx-swiper-dots {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 30rpx;
  height: 20rpx;
  line-height: 20rpx;
  text-align: center;
}
.yx-swiper-dot {
  display: inline-block;
  width: 12rpx;
  height: 12rpx;
  border-radius: 12rpx;
  margin: 0 8rpx;
  background-color: #f7f7f7;
  opacity: 0.3;
}
.yx-swiper-active-dot {
  background-color: #1c86ee;
  opacity: 0.8;
}
.yx-tab {
  display: -webkit-flex;
  display: flex;
  justify-items: stretch;
  height: 100rpx;
  font-size: 30rpx;
  overflow: hidden;
}
.yx-text {
  display: inline-block;
}
.yx-page {
  position: absolute;
  display: -webkit-flex;
  display: flex;
  flex-direction: column;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: #ffffff;
  z-index: 1;
}
.yx-header {
  display: inline-flex;
  overflow: hidden;
  width: 100%;
  height: 90rpx;
  padding-left: 20rpx;
  flex-wrap: nowrap;
  align-items: center;
  white-space: nowrap;
  border-bottom: 0.5px solid #cccccc;
  z-index: 999;
}
.yx-header-back,
.yx-header-icon,
.yx-header-space {
  flex-shrink: 0;
  flex-grow: 0;
}
.yx-header-back {
  width: 90rpx;
  margin-left: -20rpx;
  padding: 50rpx 0;
  text-align: center;
}
.yx-header-hide {
  margin-left: -20rpx;
  display: none;
}
.yx-header-icon {
  width: 60rpx;
  text-align: center;
}
.yx-header-host {
  overflow: hidden;
  flex-shrink: 1;
  flex-grow: 1;
}
.yx-host {
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  width: 100%;
  height: auto;
  font-family: "Helvetica Neue", Helvetica, STHeiTi, Arial, sans-serif;
  font-size: 32rpx;
}
.yx-layout-row {
  display: -webkit-flex;
  display: flex;
  flex-direction: row;
}
.yx-layout-row-reverse {
  display: -webkit-flex;
  display: flex;
  flex-direction: row-reverse;
}
.yx-layout-column {
  display: -webkit-flex;
  display: flex;
  flex-direction: column;
}
.yx-layout-column-reverse {
  display: -webkit-flex;
  display: flex;
  flex-direction: column-reverse;
}
.yx-layout-before {
  justify-items: flex-start;
  justify-content: flex-start;
}
.yx-layout-center {
  justify-items: center;
  justify-content: center;
}
.yx-layout-after {
  justify-items: flex-end;
  justify-content: flex-end;
}
.yx-layout-space-between {
  justify-items: space-between;
  justify-content: space-between;
}
.yx-layout-space-around {
  justify-items: space-around;
  justify-content: space-around;
}
.yx-layout-stretch {
  justify-items: stretch;
  justify-content: stretch;
}
.yx-layout-left,
.yx-layout-up {
  align-content: flex-start;
  align-items: flex-start;
}
.yx-layout-middle {
  align-content: center;
  align-items: center;
}
.yx-layout-right,
.yx-layout-down {
  align-content: flex-end;
  align-items: flex-end;
}
.yx-layout-line {
  display: block;
  white-space: nowrap;
  overflow: auto;
}
.yx-layout-line > .yx-control {
  display: inline-block;
}
.yx-layout-line > .yx-layout-row,
.yx-layout-line > .yx-layout-column,
.yx-layout-line > .yx-layout-row-reverse,
.yx-layout-line > .yx-layout-column-reverse {
  display: -webkit-inline-flex;
  display: inline-flex;
}
.yx-layout-vline {
  display: block;
  overflow: auto;
}
.yx-layout-vline > .yx-control {
  display: block;
}
.yx-layout-vline > .yx-layout-row,
.yx-layout-vline > .yx-layout-column,
.yx-layout-vline > .yx-layout-row-reverse,
.yx-layout-vline > .yx-layout-column-reverse {
  display: -webkit-flex;
  display: flex;
}
.yx-layout-flow {
  display: block;
}
.yx-layout-flow > .yx-control {
  display: inline-block;
}
.yx-layout-flow > .yx-layout-row,
.yx-layout-flow > .yx-layout-column,
.yx-layout-flow > .yx-layout-row-reverse,
.yx-layout-flow > .yx-layout-column-reverse {
  display: -webkit-inline-flex;
  display: inline-flex;
}
.yx-backstage {
  visibility: hidden;
}
.yx-hidden {
  display: none;
}
