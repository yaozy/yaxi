        class="yx-control yx-line{{ class || '' }}" 
        style="{{ s || '' }}{{ hidden ? 'display:none;' : '' }}{{ color ? 'background-color:' + color + ';' : '' }}{{ size ? 'width:' + size + ';' : '' }}"
        bindtouchstart="translateEvent" />

</template>


<template name="Vline">

